<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/custom_game/overthrow_scoreboard.vcss_c" />
	</styles>
	<scripts>
		<include src="s2r://panorama/scripts/custom_game/overthrow_scoreboard.vjs_c" />
	</scripts>
		<Panel class="VictoryPanel">
			<Panel id="AllVictoryPanel">

				<Panel id="BirzhaScore" class="PanelInformation">
					<Panel class="PanelCenter">
						<Panel id="PointToWin"	/>
						<Label class="PanelText" id="VictoryPoints" text="0" />
					</Panel>
				</Panel>
				
				<Panel id="BirzhaRewardTime" class="PanelInformation">
					<Panel class="PanelCenter">
						<Panel id="TimerReward"	/>
						<Label class="PanelText" id="Timer" text="00:00" />
					</Panel>
				</Panel>

				<Panel id="BirzhaContractTime" class="PanelInformation">
					<Panel class="PanelCenter">
						<Panel id="TimerContract"	/>
						<Label class="PanelText" id="ContractTimer" text="00:00" />
					</Panel>
				</Panel>

				<Panel id="BirzhaGameTime" class="PanelInformation">
					<Panel class="PanelCenter">
						<Panel id="TimerGame"	/>
						<Label class="PanelText" id="GameTimer" text="00:00" />
					</Panel>
				</Panel>
				
				<Panel id="BirzhaFountainTime" class="PanelInformation">
					<Panel class="PanelCenter">
						<Panel id="FountainTimerLogo"	/>
						<Label class="PanelText" id="FountainTimer" text="00:00" />
					</Panel>
				</Panel>

				<Panel id="BirzhaEnd" class="PanelInformation">
					<Panel class="PanelCenter">
						<Panel id="EndGameTimer"/>
						<Label class="PanelText" id="EndTimer" text="00:00" />
					</Panel>
				</Panel>
			</Panel>

			<Panel id="ContractPanel" >
				<Label text="#contracttargets" class="LabelText" />
				<Panel id="HeroIcons">

				</Panel>
			</Panel>

			<Panel id="CloseGameInfo" onactivate="ToggleInfo();">
				<Label id="InfoText" class="TextOpen" text="#CloseGameInfo"/>
			</Panel>
		</Panel>
</root>

